{
    "name": "saritasa/laravel-notifications-api",
    "type": "library",
    "description": "Implementation for notifications API",
    "keywords": ["laravel", "api", "notifications"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Populov",
            "email": "sergey@saritasa.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/database": "^5.4",
        "illuminate/http": "^5.4",
        "illuminate/support": "^5.4",
        "saritasa/eloquent-custom": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4" : {
            "Saritasa\\PushNotifications\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "laravel": {
            "providers": [
                "Saritasa\\PushNotifications\\NotificationsApiServiceProvider"
            ]
        }
    }
}
